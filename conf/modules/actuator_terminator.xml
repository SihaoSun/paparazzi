<!DOCTYPE module SYSTEM "module.dtd">

<module name="actuator_terminator">
  <doc>
    <description>Simple triger to terminate one actuator for damage flight test</description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings NAME="actuator_terminator">
        <dl_setting var="FDD_delay" min="0" step="0.01" max="0.3" shortname="delay" param="FDD_DELAY" persistent="true"/>
      </dl_settings>
    </dl_settings>
  </settings>
  <header>
    <file name="actuator_terminator.h"/>
  </header>
  <init fun = "actuator_terminator_init()"/>
  <periodic fun="actuator_terminator()" autorun="TRUE" stop = "actuator_terminator_init()"/>
  <!--periodic fun="actuator_terminator_print()"/-->
  <makefile>
    <file name="actuator_terminator.c"/>
  </makefile>


</module>

