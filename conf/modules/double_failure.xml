<!DOCTYPE module SYSTEM "module.dtd">

<module name="double_failure">
  <doc>
    <description>introduce a double rotor failure</description>
  </doc>
  <header>
    <file name="double_failure.h"/>
  </header>
  <init fun="double_failure_init()"/>
  <periodic fun="double_failure()" freq="512" stop="double_failure_init()" autorun="FALSE"/>
  <makefile>
    <file name="double_failure.c"/>
  </makefile>
</module>

